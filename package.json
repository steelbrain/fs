  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "ava": "^1.0.1",
    "eslint-config-steelbrain": "^6.0.0",
    "flow-bin": "^0.97.0",
    "sb-babel-cli": "^1.0.4"
  },
  "dependencies": {
    "sb-promisify": "^2.0.1",
    "strip-bom-buf": "^1.0.0"
